{
  "name": "prisma-passthrough",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node source/Main.ts --ext graphql,ts",
    "playground": "yarn graphql playground",
    "gen:schema:types": "gql-gen --schema http://localhost:4000 --template typescript --out ./source/Schema/Types.ts",
    "gen:core": "yarn gen:core:schema && yarn gen:core:binding",
    "gen:core:schema": "graphql get-schema --project core",
    "gen:core:binding": "graphql codegen --project core"
  },
  "dependencies": {
    "@types/ramda": "^0.25.34",
    "apollo-link-http": "^1.5.4",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-tools": "^3.0.2",
    "graphql-yoga": "^1.14.8",
    "node-fetch": "^2.1.2",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.1",
    "@types/node-fetch": "^2.1.1",
    "graphql-cli": "^2.16.3",
    "graphql-code-generator": "^0.9.3",
    "graphql-codegen-typescript-template": "^0.9.3",
    "nodemon": "^1.17.5",
    "prisma-binding": "^2.1.0",
    "ts-node": "^6.1.1",
    "typescript": "^2.9.2"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
